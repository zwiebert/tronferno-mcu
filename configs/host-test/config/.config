
#
# zwiebert: CLI
#
CONFIG_UOUT_HAVE_COMPONENT=y
CONFIG_CLI_JSON_ROOT_OBJECT=y
# CONFIG_CLI_CMDLINE_ALLOW_LF is not set
CONFIG_CLI_INTERACTIVE_CONSOLE=y
CONFIG_APP_USE_CLI_TASK=y
# CONFIG_CLI_DEBUG is not set
# end of zwiebert: CLI

#
# zwiebert: config_kvs
#
CONFIG_CONFIG_KVS_HAVE_COMPONENT=y
CONFIG_APP_CFG_NAMESPACE="Settings"
# end of zwiebert: config_kvs

#
# zwiebert: Firmware Updater OTA
#
CONFIG_APP_OTA_USE_CERT_BUNDLE=y
# end of zwiebert: Firmware Updater OTA

#
# zwiebert: STM32
#
CONFIG_STM32_HAVE_COMPONENT=y
CONFIG_STM32_USE_COMPONENT=y
# CONFIG_STM32_DEBUG is not set
CONFIG_STM32_ENABLE_RESET=y
CONFIG_STM32_MAKE_COMPONENT_THREAD_SAFE=y
CONFIG_STM32_MAKE_API_THREAD_SAFE=y
CONFIG_STM32_UART_PARITY_DEFAULT=0
# end of zwiebert: STM32

#
# zwiebert: txtio
#
CONFIG_TXTIO_HAVE_COMPONENT=y
CONFIG_APP_UART_BAUD_RATE=115200
# end of zwiebert: txtio

#
# zwiebert: weather
#
CONFIG_WEATHER_OWM_URL_STRING="http://api.openweathermap.org/data/2.5/weather?q=Berlin&APPID=12345678901234567890123456789012"
# CONFIG_WEATHER_TEST_DEBUG is not set
# end of zwiebert: weather

#
# zwiebert: HTTP Server
#
CONFIG_NET_HTTP_SERVER_HAVE_COMPONENT=y
CONFIG_APP_HTTP_ENABLE=0
CONFIG_APP_HTTP_USER=""
CONFIG_APP_HTTP_PASSWORD=""
# CONFIG_NET_HTTP_SERVER_DEBUG is not set
# end of zwiebert: HTTP Server

#
# zwiebert: Networking
#
CONFIG_NET_HAVE_COMPONENT=y
CONFIG_APP_TCPS_ENABLE=1
CONFIG_APP_TCPS_PORT=7777
CONFIG_APP_TCPS_PORT_IA=0
CONFIG_APP_TCPS_CONNECTIONS_MAX=-1
# CONFIG_NET_HTTP_CLIENT_DEBUG is not set
# CONFIG_NET_NTP_CLIENT_DEBUG is not set
# CONFIG_NET_TCP_CLI_CLIENT_DEBUG is not set
# CONFIG_NET_NETWORK_DEBUG is not set
# end of zwiebert: Networking

#
# zwiebert: MQTT client
#
CONFIG_NET_MQTT_CLIENT_HAVE_COMPONENT=y
CONFIG_APP_MQTT_CLIENT_ID="zb"
CONFIG_APP_MQTT_ROOT_TOPIC="root"
CONFIG_APP_MQTT_URL=""
CONFIG_APP_MQTT_ENABLE=0
CONFIG_APP_MQTT_USER=""
CONFIG_APP_MQTT_PASSWORD=""
# CONFIG_NET_MQTT_CLIENT_DEBUG is not set
# end of zwiebert: MQTT client

#
# zwiebert: UOUT
#
# CONFIG_UOUT_PUBLISH_LOG_AS_TXT is not set
CONFIG_UOUT_PUBLISH_IP_ADDR_AS_TXT=y
CONFIG_REQUIRE_ROOT_OBJECT=y
# CONFIG_UOUT_DEBUG is not set
# end of zwiebert: UOUT

#
# zwiebert: Tronferno Transceiver
#
CONFIG_APP_USE_FER_TRANSMITTER=y
CONFIG_APP_USE_FER_RECEIVER=y
# CONFIG_TRONFERNO_TRX_DEBUG is not set
# end of zwiebert: Tronferno Transceiver

#
# zwiebert: Tronferno App Library
#
CONFIG_APP_USE_CUAS=y
CONFIG_APP_USE_SEP=y
# CONFIG_APP_USE_TIMER_DATA_FS is not set
CONFIG_APP_USE_TIMER_DATA_KVS=y
CONFIG_APP_USE_PAIRINGS=y
CONFIG_APP_USE_REPEATER=y
CONFIG_APP_USE_RTC_AUTO_UPD=y
# CONFIG_FERNOTRON_APP_DEBUG is not set
# end of zwiebert: Tronferno App Library

